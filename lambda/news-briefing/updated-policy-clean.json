{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPublicReadBarsTiersCharts",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": [
        "arn:aws:s3:::tradespark-822233328169-us-east-1/bars/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/tiers/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/charts/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/summary_json/*"
      ]
    },
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/summary_json/*"
    },
    {
      "Sid": "AllowPublicReadOptiSpark",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": [
        "arn:aws:s3:::tradespark-822233328169-us-east-1/bars/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/charts/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/tiers/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/weekly/*",
        "arn:aws:s3:::tradespark-822233328169-us-east-1/summary_json/*"
      ]
    },
    {
      "Sid": "DenyPublicReadDB",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/db/*",
      "Condition": {
        "StringNotEquals": {
          "aws:PrincipalAccount": "822233328169"
        }
      }
    },
    {
      "Sid": "AllowLambdaDbGet",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::822233328169:role/tradespark-lambda-role"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/db/*"
    },
    {
      "Sid": "AllowLambdaDbList",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::822233328169:role/tradespark-lambda-role"
      },
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "db/",
            "db/*"
          ]
        }
      }
    },
    {
      "Sid": "AllowDbGet-tradespark-lambda-exec",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::822233328169:role/tradespark-lambda-exec"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/db/*"
    },
    {
      "Sid": "AllowDbList-tradespark-lambda-exec",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::822233328169:role/tradespark-lambda-exec"
      },
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1",
      "Condition": {
        "StringLike": {
          "s3:prefix": [
            "db/",
            "db/*"
          ]
        }
      }
    },
    {
      "Sid": "AllowGetBars",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/bars/*"
    },
    {
      "Sid": "AllowLambdaBasicExecWriteSummaryJson",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::822233328169:role/lambda-basic-exec"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/summary_json/*"
    },
    {
      "Sid": "AllowPublicReadBriefings",
      "Effect": "Allow",
      "Principal": "*",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::tradespark-822233328169-us-east-1/briefings/*"
    }
  ]
}